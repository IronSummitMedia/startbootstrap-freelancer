    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "5.6.3",
        "bootstrap": "4.2.1",
        "jquery": "3.3.1",
        "jquery.easing": "^1.4.1",
        "magnific-popup": "^1.1.0"
    },
    "devDependencies": {
